    "@types/pako": "2.0.3",
    "@types/prismjs": "1.26.4",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "7.14.1",
    "@typescript-eslint/parser": "7.13.1",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",

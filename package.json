{
  "name": "polkadot-devground",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "add-polkadot": "npx papi add dot -n polkadot",
    "add-polkadot-people": "npx papi add dotpeople -n polkadot_people",
    "add-rococo": "npx papi add rococo -n rococo_v2_2",
    "add-rococo-people": "npx papi add rococo_people -n rococo_v2_2_people",
    "add-descriptors": "npm run add-polkadot && npm run add-polkadot-people && npm run add-rococo && npm run add-rococo-people",
    "remove-descriptors": "rm -rf node_modules .papi",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "preview": "vite preview",
    "start": "vite"
  },
  "dependencies": {
    "@pivanov/event-bus": "1.0.0",
    "@pivanov/use-toggle-visibility": "0.0.1",
    "@polkadot-api/descriptors": "file:.papi/descriptors",
    "@polkadot-api/view-builder": "0.3.3",
    "@polkadot/types": "12.2.3",
    "@polkadot/util": "13.0.2",
    "@radix-ui/react-scroll-area": "1.1.0",
    "@radix-ui/react-switch": "1.1.0",
    "@shikijs/monaco": "1.10.0",
    "@tanstack/react-virtual": "3.8.2",
    "@typescript/ata": "0.9.6",
    "@vitejs/plugin-react-swc": "3.7.0",
    "axios": "1.7.2",
    "clsx": "2.1.1",
    "date-fns": "^3.6.0",
    "file-saver": "2.0.5",
    "jszip": "3.10.1",
    "lodash.debounce": "4.0.8",
    "monaco-editor": "0.50.0",
    "nanoid": "5.0.7",
    "pako": "2.1.0",
    "polkadot-api": "0.13.2",
    "prettier": "3.3.2",
    "prism-themes": "1.9.0",
    "prismjs": "1.29.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-helmet-async": "2.0.5",
    "react-hot-toast": "2.4.1",
    "react-resizable-panels": "2.0.19",
    "react-router-dom": "6.24.0",
    "scale-ts": "1.6.0",
